{
  "name": "mini_web_app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run build:watch & npm run serve",
    "build": "esbuild src/main.tsx --bundle --outfile=dist/bundle.js --format=esm --target=es2020 --jsx=automatic --loader:.css=css",
    "build:watch": "npm run build -- --watch",
    "serve": "serve dist -p 3000",
    "lint": "eslint ."
  },
  "dependencies": {
    "@telegram-apps/sdk-react": "^3.3.7",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "mime": "^4.1.0",
    "openai": "^5.21.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "esbuild": "^0.25.10",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "serve": "^14.2.5",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0"
  }
}
